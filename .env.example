# .env.example
# Copiez ce fichier vers .env et remplissez vos valeurs

# Clé privée du wallet déployeur (SANS le préfixe 0x)
# ATTENTION: Ne jamais commiter la vraie clé privée !
PRIVATE_KEY=your_private_key_here_without_0x_prefix

# Clé API BSCScan pour la vérification des contrats
# Obtenez-la sur https://bscscan.com/apis
BSCSCAN_API_KEY=your_bscscan_api_key_here

# Clé API CoinMarketCap pour le gas reporter (optionnel)
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key_here

# Activer le rapport de gas (optionnel)
REPORT_GAS=false

# Configuration RPC personnalisée (optionnel)
BSC_RPC_URL=https://bsc-dataseed.binance.org/
BSC_TESTNET_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545/

# Adresses importantes (pour référence)
TREASURY_WALLET=0xe0B7BCb42aeBB5Be565cc81518a48D780f3c001c
USDC_BSC_MAINNET=0x8AC76a51cc950d9822D68b83fE1Ad97B32Cd580d
USDC_BSC_TESTNET=0x64544969ed7EBf5f083679233325356EbE738930